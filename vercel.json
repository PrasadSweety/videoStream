{
  "builds": [
    {
      "src": "backend/netflix/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "backend/netflix/build"
      }
    },
    {
      "src": "backend/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/index.js"
    },
    {
      "src": "/(.*)",
      "dest": "backend/netflix/build/$1"
    },
    {
      "handle": "filesystem"
    }
  ]
}
